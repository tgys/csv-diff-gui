SELECT *
FROM (SELECT t1.[CW Hierarchy], t2.[CW Hierarchy],t1.[Section], t2.[Section], t1.[Milestone Date], t2.[Milestone Date], t1.[District], t2.[District], t1.[Cluster], t2.[Cluster], t1.[FW Hierarchy], t2.[FW Hierarchy], t1.[CT Hierarchy], t2.[CT Hierarchy], t1.[KB Hierarchy], t2.[KB Hierarchy], t1.[VG Hierarchy], t2.[VG Hierarchy], t1.[Recorded Length], t2.[Recorded Length], t1.[Inventory Length], t2.[Inventory Length], t1.[NCI], t2.[NCI], t1.[FWCI], t2.[FWCI], t1.[CTCI], t2.[CTCI], t1.[KBCI], t2.[KBCI], t1.[VGCI], t2.[VGCI], t1.[PCI], t2.[PCI], t1.[SRI], t2.[SRI], t1.[CW SCI], t2.[CW SCI], t1.[FW SCI], t2.[FW SCI], t1.[CT SCI], t2.[CT SCI], t1.[KB SCI], t2.[KB SCI], t1.[VG SCI], t2.[VG SCI] FROM t1 left join t2 on (t1.[CW Hierarchy] = t2.[CW Hierarchy]) AND (t1.Section = t2.Section) union SELECT t1.[CW Hierarchy], t2.[CW Hierarchy],t1.[Section], t2.[Section], t1.[Milestone Date], t2.[Milestone Date], t1.[District], t2.[District], t1.[Cluster], t2.[Cluster], t1.[FW Hierarchy], t2.[FW Hierarchy], t1.[CT Hierarchy], t2.[CT Hierarchy], t1.[KB Hierarchy], t2.[KB Hierarchy], t1.[VG Hierarchy], t2.[VG Hierarchy], t1.[Recorded Length], t2.[Recorded Length], t1.[Inventory Length], t2.[Inventory Length], t1.[NCI], t2.[NCI], t1.[FWCI], t2.[FWCI], t1.[CTCI], t2.[CTCI], t1.[KBCI], t2.[KBCI], t1.[VGCI], t2.[VGCI], t1.[PCI], t2.[PCI], t1.[SRI], t2.[SRI], t1.[CW SCI], t2.[CW SCI], t1.[FW SCI], t2.[FW SCI], t1.[CT SCI], t2.[CT SCI], t1.[KB SCI], t2.[KB SCI], t1.[VG SCI], t2.[VG SCI] FROM t1 right join t2 on (t1.[CW Hierarchy] = t2.[CW Hierarchy]) AND (t1.Section = t2.Section) )  AS [%$##@_Alias]
WHERE ((t1.[Milestone Date]<>t2.[Milestone Date] Or t1.District<>t2.District Or t1.Cluster<>t2.Cluster Or t1.[FW Hierarchy]<>t2.[FW Hierarchy] Or t1.[CT Hierarchy]<>t2.[CT Hierarchy] Or t1.[KB Hierarchy]<>t2.[KB Hierarchy] Or t1.[VG Hierarchy]<>t2.[VG Hierarchy] Or t1.[Recorded Length]<>t2.[Recorded Length] Or t1.[Inventory Length]<>t2.[Inventory Length] Or t1.NCI<>t2.NCI Or t1.FWCI<>t2.FWCI Or t1.CTCI<>t2.CTCI Or t1.KBCI<>t2.KBCI Or t1.VGCI<>t2.VGCI Or t1.PCI<>t2.PCI Or t1.SRI<>t2.SRI Or t1.[CW SCI]<>t2.[CW SCI] Or t1.[FW SCI]<>t2.[FW SCI] Or t1.[CT SCI]<>t2.[CT SCI] Or t1.[KB SCI]<>t2.[KB SCI] Or t1.[VG SCI]<>t2.[VG SCI])) Or (t1.[CW Hierarchy] Is Null And t1.Section Is Null) Or (t2.[CW Hierarchy] Is Null And t2.Section Is Null);
